@using MvcProject.Models.Entity
@model List<About>

@foreach (var x in Model)
{
    <section class="resume-section" id="about">
        <div class="resume-section-content">
            <h1 class="mb-0">
                @x.Name
                <span class="text-info">@x.SurName</span>
            </h1>
            <div class="subheading mb-5">
                @x.Subtitle ·
                <a class="text-info" href="mailto:@x.Mail">@x.Mail</a>
            </div>
            <p class="lead mb-5">@x.Content</p>
            @Html.Action("SocialMedia")
        </div>
    </section>
}

